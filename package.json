{
  "name": "node-service-starter-ts",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "run-s clean && tsc-watch --onSuccess 'npm run start-server'",
    "start-server": "env-cmd -f .env node build/index.js",
    "clean": "rm -rf build"
  },
  "author": "Cheri Hung",
  "license": "MIT",
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "tsc-watch": "^6.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  }
}